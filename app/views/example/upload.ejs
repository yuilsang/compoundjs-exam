<%-include ../layouts/header%>
<style>
h1.title {
	font-size:50px;
}

.alert-message .block-message .info {
	background-color: #ddf4fb;
	border-color: #c6edf9;
}

.alert-message {
	color: #404040;
	text-shadow: 0 1px 0 rgba(255,255,255,0.5);
}
</style>
<div class="container-narrow">
	<div class="masthead">
	<%-include ../layouts/menu%>

	<h3 class="muted">compoundjs-exam</h3>
	</div>
	<hr>

	<section>
		<h3>Uploaded File List</h3>
		<div class="_filelist bs-docs-example" style="padding-bottom: 24px;">
			<ul id="filelist">
			</ul>
		</div>

		<h3>File Upload</h3>
		<div class="_fileupload bs-docs-example" style="padding-bottom: 24px;">
			<span class="_uploadfilename"></span>
			<% var form = formFor("upload_test", { action: "/upload/filesave", method: "post", enctype: "multipart/form-data" }); %>
			<%- form.begin() %>
			<input name="file" id="file" type="file" value="" style="width:60px;height:25px" />
			<br><br>
			<button type="submit" class="btn btn-large btn-primary">upload</button>
			<%- form.end() %>
		</div>

		<h3>HTML5 Upload</h3>
		<div class="_html5upload bs-docs-example" style="padding-bottom: 24px;">
			<a href="#" class="thumbnail" style="width:360px" id="dropbox">
			<img style="width: 360px; height: 270px;" src="/img/upload.png">
			</a>
			<br>
			<div class="progress active" style="width:370px">
            <div class="bar" style="width: 0%;"></div>
            </div>

			파일을 이미지 위에 드래그 드롭해주세요
		</div>
	</section>

	<div class="footer">
	<p>&copy; yuilsang 2013</p>
	</div>
</div>
<%-include ../layouts/footer%>